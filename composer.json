{
    "name": "drupal-composer/drupal-project",
    "description": "Associated Process Server App",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "dresden",
            "role": "Lord of the Earth"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=8.1.31",
        "commerceguys/addressing": "1.0.9",
        "composer/installers": "^1.0.24",
        "composer/semver": "3.3.2",
        "cweagans/composer-patches": "^1.6",
        "dompdf/dompdf": "2.0.8",
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^2.3",
        "drupal/adminimal_admin_toolbar": "^1.8",
        "drupal/adminimal_theme": "^1.4",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/chosen": "^2.6",
        "drupal/conditional_fields": "4.0.0-alpha2",
        "drupal/console": "^1.0.2",
        "drupal/core": "^9.4.0",
        "drupal/core-composer-scaffold": "9.4",
        "drupal/core-project-message": "9.4",
        "drupal/core-recommended": "9.4",
        "drupal/csv_serialization": "3.x-dev@dev",
        "drupal/ctools": "^3.0",
        "drupal/datetimehideseconds": "^1.0",
        "drupal/easy_breadcrumb": "^1.13",
        "drupal/entity_print": "2.15",
        "drupal/entity_reference_revisions": "^1.6",
        "drupal/entity_usage": "^2.0@alpha",
        "drupal/field_formatter": "^3.0.0",
        "drupal/field_formatter_class": "^1.1",
        "drupal/field_group": "^3.0",
        "drupal/field_permissions": "^1.0",
        "drupal/git_deploy": "^2.6",
        "drupal/ief_table_view_mode": "^2.0@beta",
        "drupal/inline_entity_form": "^1.0",
        "drupal/mailgun": "^1.0@RC",
        "drupal/markup": "^1.0@beta",
        "drupal/node_save_redirect": "1.x-dev",
        "drupal/paragraphs": "^1.5",
        "drupal/pathauto": "^1.3",
        "drupal/role_login_page": "^2.0.3",
        "drupal/rules": "^3.0@alpha",
        "drupal/select_or_other": "1.x-dev",
        "drupal/simple_multistep": "^2.1.0",
        "drupal/smtp": "^1.0@beta",
        "drupal/token": "^1.5",
        "drupal/twig_tweak": "^2.3",
        "drupal/typed_data": "^1.0@alpha",
        "drupal/upgrade_status": "^3.19",
        "drupal/views_data_export": "^1.6",
        "drupal/xmlrpc": "^1.0@beta",
        "drush/drush": "11",
        "laminas/laminas-diactoros": "2.11.0",
        "laminas/laminas-zendframework-bridge": "^1.2",
        "myclabs/deep-copy": "1.11",
        "nyholm/psr7": "1.4",
        "php-http/message": "1.6",
        "psr/http-message": "1.0.1",
        "symfony-cmf/routing": "^2.3",
        "symfony/cache": "^6.3",
        "symfony/console": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/event-dispatcher": "^4.4",
        "symfony/http-foundation": "^4.4.7",
        "symfony/process": "^4.4",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/serializer": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/yaml": "^4.4.19",
        "twig/twig": "2.15.3",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^2.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true,
            "zaporylie/composer-drupal-optimizations": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "enable-patching": true,
        "patches": {
            "drupal/simple_multistep": {
                "Simple MultiStep": "patches/step_redirection.patch"
            }
        }
    }
}
