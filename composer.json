{
    "name": "drupal-composer/drupal-project",
    "description": "Associated Process Server App",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "dresden",
            "role": "Lord of the Earth"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=8.2.28",
        "commerceguys/addressing": "2.1.1",
        "composer/installers": "^1.0.24",
        "composer/semver": "^3.4.3",
        "cweagans/composer-patches": "^1.6",
        "dompdf/dompdf": "2.0.8",
        "drupal/address": "^2.0.4",
        "drupal/admin_toolbar": "^3.6.2",
        "drupal/adminimal_admin_toolbar": "^2.0.0",
        "drupal/adminimal_theme": "^1.7",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/chosen": "^5.0",
        "drupal/chosen_field": "4.0",
        "drupal/chosen_lib": "5.0",
        "drupal/ckeditor": "1.0",
        "drupal/conditional_fields": "^4.0.0-alpha6",
        "drupal/core-composer-scaffold": "10.5",
        "drupal/core-project-message": "10.5",
        "drupal/core-recommended": "10.5",
        "drupal/csv_serialization": "3.0",
        "drupal/ctools": "^4.1",
        "drupal/datetimehideseconds": "^1.0",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/entity_print": "2.18",
        "drupal/entity_reference_revisions": "^1.12",
        "drupal/entity_usage": "2.0.0-beta17",
        "drupal/field_formatter": "3.0",
        "drupal/field_formatter_class": "^1.8",
        "drupal/field_permissions": "^1.4",
        "drupal/git_deploy": "^2.6",
        "drupal/ief_table_view_mode": "^3.0",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/mailgun": "^2.1",
        "drupal/markup": "^2.0",
        "drupal/node_save_redirect": "2.0",
        "drupal/paragraphs": "^1.19",
        "drupal/pathauto": "^1.13",
        "drupal/role_login_page": "^2.0.3",
        "drupal/rules": "^4.0",
        "drupal/select_or_other": "4.2",
        "drupal/seven": "2.0.0-beta1",
        "drupal/simple_multistep": "*",
        "drupal/smtp": "^1.4",
        "drupal/twig_field_value": "2.0.5",
        "drupal/twig_tweak": "^3.4",
        "drupal/typed_data": "^2.1",
        "drupal/views_data_export": "1.5",
        "drupal/xmlrpc": "^1.0@beta",
        "drush/drush": "13.6.2",
        "guzzlehttp/promises": "2.2",
        "laminas/laminas-diactoros": "3.6.0",
        "laminas/laminas-zendframework-bridge": "^1.2",
        "myclabs/deep-copy": "1.11",
        "nyholm/psr7": "1.8.2",
        "php-http/message": "1.16.2",
        "psr/http-message": "2.0",
        "psr/log": "^3.0",
        "symfony-cmf/routing": "^3.0",
        "symfony/cache": "^7.3.2",
        "symfony/console": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/event-dispatcher": "^6.4",
        "symfony/http-foundation": "^6.4.21",
        "symfony/process": "^6.4",
        "symfony/psr-http-message-bridge": "^6.4",
        "symfony/serializer": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/yaml": "^6.4.21",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^2.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true,
            "zaporylie/composer-drupal-optimizations": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "enable-patching": true,
        "patches": {
            "drupal/simple_multistep": {
                "Simple MultiStep": "patches/step_redirection.patch"
            }
        }
    }
}
